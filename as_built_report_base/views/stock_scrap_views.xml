<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Tracebility rename in scrap -->
    <record id="stock_scrap_form_view_inherit_as_built_report_base" model="ir.ui.view">
        <field name="name">stock.scrap.form.inherit_as_built_report_base</field>
        <field name="model">stock.scrap</field>
        <field name="inherit_id" ref="stock.stock_scrap_form_view"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='action_get_stock_move_lines']" position="attributes">
                <attribute name="string">As-Built Traceability</attribute>
            </xpath>
        </field>
    </record>
</odoo>
