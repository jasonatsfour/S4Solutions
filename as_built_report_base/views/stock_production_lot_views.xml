<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Tracebility rename in stock production lot -->
    <record id="view_production_lot_form_inherit_as_built_report_base" model="ir.ui.view">
        <field name="name">stock.production.lot.form.inherit_as_built_report_base</field>
        <field name="model">stock.production.lot</field>
        <field name="inherit_id" ref="stock.view_production_lot_form"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='%(stock.action_stock_report)d']" position="attributes">
                <attribute name="string">As-Built Traceability</attribute>
            </xpath>
        </field>
    </record>
</odoo>
