<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Tracebility rename in stock picking -->
    <record id="view_picking_form_inherit_as_built_report_base" model="ir.ui.view">
        <field name="name">stock.picking.form.inherit_as_built_report_base</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//button[@name='%(stock.action_stock_report)d']" position="attributes">
                <attribute name="string">As-Built Traceability</attribute>
            </xpath>
        </field>
    </record>
</odoo>
