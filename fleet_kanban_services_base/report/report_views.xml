<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <!-- Paperformat for KFM report -->
    <record id="paperformat_vehicle_barcode_report" model="report.paperformat">
        <field name="name">Vehicle Barcode</field>
        <field name="format">custom</field>
        <field name="page_height">32</field>
        <field name="page_width">60</field>
        <field name="orientation">Portrait</field>
        <field name="margin_top">2</field>
        <field name="margin_bottom">2</field>
        <field name="margin_left">5</field>
        <field name="margin_right">5</field>
        <field name="header_line" eval="False"/>
        <field name="header_spacing">10</field>
        <field name="dpi">94</field>
    </record>

    <report string="Vehicle Barcode"
            id="action_fleet_report_vehicle_barcode"
            model="fleet.vehicle"
            report_type="qweb-pdf"
            name="fleet_kanban_services_base.report_fleet_vehicle_barcode"
            file="fleet_kanban_services_base.report_fleet_vehicle_barcode"
            paperformat="fleet_kanban_services_base.paperformat_vehicle_barcode_report"/>
</odoo>
