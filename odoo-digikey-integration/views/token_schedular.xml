<odoo>
    
    <data noupdate="1">
        <record id="digikey_scheduler_refresh_token_action" model="ir.cron">
            <field name="name">Digikey Refresh Token Scheduler</field>
            <field name="model_id" ref="model_digikey_connector"/>
            <field name="user_id" ref="base.user_root"/>
            <field name="active">True</field>
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="state">code</field>
            <field name="code">model.refresh_connection()</field>
            <field  name="doall" eval="False"/>
            <field name="user_id" ref="base.user_root"/>
            
        </record>

   
</data>
</odoo>