<?xml version="1.0" encoding="utf-8"?>
<odoo>
<data noupdate="1">
	<record model="ir.module.category" id="module_billdotcom_odoo_bridge_category">
		<field name="name">Bill.com ODOO Bridge</field>
		<field name="description">Bill.com Odoo Bridge Application</field>
		<field name="sequence">20</field>
    </record>
	
	<record id="group_billdotcom_odoo_user" model="res.groups">
		<field name="name">User</field>
		<field name="comment">User will have an read access to the wp odoo bridge.</field>
		<field name="category_id" ref="module_billdotcom_odoo_bridge_category"/>
	</record>
	
    <record id="group_billdotcom_odoo_manager" model="res.groups">
        <field name="name">Manager</field>
		<field name="comment">Full Access on Wp Odoo Bridge</field>
		<field name="category_id" ref="module_billdotcom_odoo_bridge_category"/>
        <field name="users" eval="[(4, ref('base.user_root'))]"/>
		 <field name="implied_ids" eval="[(4, ref('group_billdotcom_odoo_user'))]"/>
    </record>
</data>
</odoo>
