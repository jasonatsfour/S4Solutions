<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_company_form_inherit_manufacturing_traceability_base" model="ir.ui.view">
        <field name="name">res.company.form.inherit_manufacturing_traceability_base</field>
        <field name="model">res.company</field>
        <field name="inherit_id" ref="base.view_company_form"/>
        <field name="arch" type="xml">
            <xpath expr="//notebook[last()]" position="inside">
                <page string="Location" name="geo_location">
                    <group>
                        <field name="supplier_location_id"/>
                    </group>
                </page>
            </xpath>
        </field>
    </record>
</odoo>
